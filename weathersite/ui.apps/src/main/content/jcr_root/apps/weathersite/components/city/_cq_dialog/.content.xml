<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"          jcr:primaryType="nt:unstructured"
          jcr:title="Author Bio"
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[aemgeeks.components.author.editor]">
    <content
            granite:class="cmp-author_editor"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <undergroundwaterlevel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                            composite="{Boolean}true"
                            fieldDescription="GroundWater Details"
                            fieldLabel="GroundWater Details"
                            required="{Boolean}false">
                        <field
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                emptyText="GroundWater Details"
                                name="./undergroundwaterlevel">
                            <items jcr:primaryType="nt:unstructured">
                                <city
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        emptyText="City Name"
                                        fieldLabel="City Name"
                                        name="./cityname"/>
                                <minlevel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        emptyText="Minimum Waterlevel "
                                        fieldLabel="Minimum Waterlevel"
                                        name="./minwaterlevel"/>
                                <maxlevel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        emptyText="Maximum Waterlevel"
                                        fieldLabel="Maximum Waterlevel"
                                        name="./maxwaterlevel"/>
                            </items>
                        </field>
                    </undergroundwaterlevel>
                </items>
            </column>
        </items>
    </content>
</jcr:root>